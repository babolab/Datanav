          <gmd:title xsi:type="gmd:PT_FreeText_PropertyType">
            <gco:CharacterString>Limite d'espace maritime revendiquée par la France en l'absence d'accord de délimitation</gco:CharacterString>
            <gmd:PT_FreeText>
              <gmd:textGroup>
                <gmd:LocalisedCharacterString locale="#EN">Maritime limits unilaterally claimed</gmd:LocalisedCharacterString>
